<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.voiddog.android.sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DampScrollActivity" />
        <activity android:name=".OverScrollAppBarActivity" />
        <activity
            android:name=".NestedDampLayoutActivity"
            android:label="@string/title_activity_nested_damp_layout"
            android:theme="@style/AppTheme" />
        <activity android:name=".SwipeRefreshActivity"></activity>
    </application>

</manifest>